<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:theme="@style/EmlakMetinStili">

    <!--Başlık-->
    <include layout="@layout/ust_kisim" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.sinasalik.thrashead.emlak.Emlak.EmlakKontroller.EmlakListView
            android:id="@+id/emlakListView"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:listeCizgili="true"
            app:listeBaslik="Haberler">

        </com.sinasalik.thrashead.emlak.Emlak.EmlakKontroller.EmlakListView>

    </ScrollView>
</LinearLayout>